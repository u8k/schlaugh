<pageAndDateNav id="<%- kind %>-page-and-date-nav">
  <text id="<%- kind %>-tag-display" class="removed"></text><br>
  <button id="author-clear-tag-<%- kind %>" onclick="clearAuthorTagButton()" class="removed">clear tag</button>
  <div id="<%- kind %>-date-box" class="no-select removed">
    <icon id="<%- kind %>-left-date-arrow" class="clicky" onclick="moveDateByOneDay(-1)">🞀</icon>
    <text id="<%- kind %>-date-display"></text>
    <icon id="<%- kind %>-right-date-arrow" class="clicky" onclick="moveDateByOneDay(1)">🞂</icon>
  </div>


  <div id="<%- kind %>-page-box" class="removed">
    <icon id="<%- kind %>-left-page-arrow" class="clicky" onclick="pageJump(-1)">🞀</icon>
    <text class="monospace">page</text>
    <icon id="<%- kind %>-right-page-arrow" class="clicky" onclick="pageJump(1)">🞂</icon>

    <form action="javascript:void(0);" id="<%- kind %>-page-info">
      <input class="monospace post-background page-number-left" id="<%- kind %>-page-number-left-input" onclick="$('<%- kind %>-page-jump').classList.remove('hidden');" onmousedown="$('<%- kind %>-page-jump').classList.remove('hidden');">
      <text class="monospace" id="<%- kind %>-page-number-left-text">7</text>
      <text class="monospace">of</text>
      <text class="monospace" id="<%- kind %>-page-number-right">7</text>
      <br><button class="hidden" id="<%- kind %>-page-jump" type="submit" onclick="pageJump(null, '<%- kind %>')">jump</button>
    </form>
  </div>
</pageAndDateNav>
