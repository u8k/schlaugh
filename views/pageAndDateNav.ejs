<pageAndDateNav id="<%- kind %>-page-and-date-nav">
  <div id="<%- kind %>-tag-display" class="removed"></div>
  <button id="author-clear-tag-<%- kind %>" onclick="clearAuthorTagButton()" class="removed">clear tag</button>
  <div id="<%- kind %>-date-box" class="no-select removed">
    <svg id="<%- kind %>-date-box" class="svg-inline--fa fa-caret-left fa-w-6 clicky" title="previous day" onclick="moveDateByOneDay(-1)" aria-labelledby="svg-inline--fa-title-1" data-prefix="fas" data-icon="caret-left" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 192 512" data-fa-i2svg=""><title id="svg-inline--fa-title-1">previous day</title><path fill="currentColor" d="M192 127.338v257.324c0 17.818-21.543 26.741-34.142 14.142L29.196 270.142c-7.81-7.81-7.81-20.474 0-28.284l128.662-128.662c12.599-12.6 34.142-3.676 34.142 14.142z"></path></svg>
    <text id="<%- kind %>-date-display"></text>
    <svg id="<%- kind %>-right-date-arrow" class="svg-inline--fa fa-caret-right fa-w-6 clicky" title="next day" onclick="moveDateByOneDay(1)" aria-labelledby="svg-inline--fa-title-7" data-prefix="fas" data-icon="caret-right" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 192 512" data-fa-i2svg=""><title id="svg-inline--fa-title-7">next day</title><path fill="currentColor" d="M0 384.662V127.338c0-17.818 21.543-26.741 34.142-14.142l128.662 128.662c7.81 7.81 7.81 20.474 0 28.284L34.142 398.804C21.543 411.404 0 402.48 0 384.662z"></path></svg>
  </div>


  <div id="<%- kind %>-page-box" class="removed">
    <svg id="<%- kind %>-left-page-arrow" class="svg-inline--fa fa-caret-left fa-w-6 clicky" title="previous page" onclick="pageJump(-1)" aria-labelledby="svg-inline--fa-title-11" data-prefix="fas" data-icon="caret-left" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 192 512" data-fa-i2svg=""><title id="svg-inline--fa-title-11">previous page</title><path fill="currentColor" d="M192 127.338v257.324c0 17.818-21.543 26.741-34.142 14.142L29.196 270.142c-7.81-7.81-7.81-20.474 0-28.284l128.662-128.662c12.599-12.6 34.142-3.676 34.142 14.142z"></path></svg>
    <text class="monospace">page</text>
    <svg id="<%- kind %>-right-page-arrow" class="svg-inline--fa fa-caret-right fa-w-6 clicky" title="next page" onclick="pageJump(1)" aria-labelledby="svg-inline--fa-title-12" data-prefix="fas" data-icon="caret-right" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 192 512" data-fa-i2svg=""><title id="svg-inline--fa-title-12">next page</title><path fill="currentColor" d="M0 384.662V127.338c0-17.818 21.543-26.741 34.142-14.142l128.662 128.662c7.81 7.81 7.81 20.474 0 28.284L34.142 398.804C21.543 411.404 0 402.48 0 384.662z"></path></svg>

    <form action="javascript:void(0);" id="<%- kind %>-page-info">
      <input class="monospace post-background page-number-left" id="<%- kind %>-page-number-left-input" onclick="$('<%- kind %>-page-jump').classList.remove('hidden');" onmousedown="$('<%- kind %>-page-jump').classList.remove('hidden');">
      <text class="monospace" id="<%- kind %>-page-number-left-text">7</text>
      <text class="monospace">of</text>
      <text class="monospace" id="<%- kind %>-page-number-right">7</text>
      <br><button class="hidden" id="<%- kind %>-page-jump" type="submit" onclick="pageJump(null, '<%- kind %>')">jump</button>
    </form>
  </div>
</pageAndDateNav>
